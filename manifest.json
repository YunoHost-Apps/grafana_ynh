{
    "name": "Grafana",
    "id": "grafana",
    "packaging_format": 1,
    "description": {
        "en": "Metric & analytic dashboards for monitoring",
        "fr": "Tableaux de bords de supervision"
    },
    "version": "8.0.6~ynh1",
    "license": "AGPL-3.0-only",
    "url": "https://grafana.com/oss/grafana/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://grafana.com/",
        "demo": "https://play.grafana.org",
        "code": "https://github.com/grafana/"
    },
    "maintainer": {
        "name": "JimboJoe",
        "email": "jimmy@monin.net",
        "url": ""
    },
    "requirements": {
        "yunohost": ">= 4.2.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/grafana",
                "default": "/grafana"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": false
            }
        ]
    }
}
